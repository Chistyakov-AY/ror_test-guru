<h3>All Users</h3>

<table>
  <thead>
    <tr>
      <th> Id </th>
      <th> Title </th>
      <th> Email </th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.title %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

