<tr>
  <td><%= test.id %></td>
  <td><%= link_to test.title, test_path(test) %> (<%= test.questions.count %>) </td> 
  <td><%= test.level %></td>
  <td><%= test.created_at %></td>
  <td><%= link_to 'Edit', edit_test_path(test) %></td> 
  <td><%= link_to 'Delete',
                test_path(test),
                method: :delete,
                data: { confirm: 'Are you sure?' } %>
  <td><%= button_to 'Start', start_test_path(test) %></td>
</tr>