<h1> All Questions: </h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>Body</th>
      <th>test_id</th>
      <th>created_at</th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.id %></td>
        <td><%= question.body%></td>
        <td><%= question.test_id %></td>
        <td><%= question.created_at %></td>
      </tr>
    <% end %>
  </tbody>

</table>
<p>
<%= link_to 'Back', admin_tests_path %>
</p>
