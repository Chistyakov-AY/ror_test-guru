<tr>
  <td><%= answer.id %></td>
  <td><%= answer.body%></td>
  <td><%= answer.correct %></td>
  <td><%= answer.created_at %></td>
  <td><%= link_to 'Edit', edit_admin_answer_path(answer) %></td> 
  <td><%= link_to 'Delete',
                admin_answer_path(answer),
                method: :delete,
                data: { confirm: 'Are you sure?' } %>
</tr>